{
  "name": "andrea-corinti-site",
  "version": "1.2.2",
  "description": "Andrea Corinti Personal Website",
  "author": "Andrea Corinti",
  "license": "MIT",
  "scripts": {
    "gen": "gulp generate",
    "start": "gulp build & gulp watch & ELEVENTY_ENV=dev eleventy --serve & yarn run start:lambda",
    "prime": "gulp build & ELEVENTY_ENV=prime eleventy",
    "build": "gulp build & ELEVENTY_ENV=prod eleventy && yarn run build:lambda",
    "dryrun": "gulp build & ELEVENTY_ENV=dev eleventy",
    "build:lambda": "netlify-lambda build src/lambda",
    "start:lambda": "netlify-lambda serve src/lambda"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andreacorinti/andreacorinti.com"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-plugin-rss": "^1.0.3",
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.0",
    "axios": "^0.18.1",
    "core-js": "3",
    "dotenv": "^6.1.0",
    "gulp": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-sass": "^4.0.1",
    "gulp-uglify": "^3.0.1",
    "html-minifier": "^3.5.20",
    "markdown-it-footnote": "^3.0.2",
    "netlify-lambda": "^1.1.0"
  },
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  }
}
