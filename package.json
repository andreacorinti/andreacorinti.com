  "repository": {
    "type": "git",
    "url": "https://github.com/andreacorinti/andreacorinti.com"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.7.1",
    "@11ty/eleventy-plugin-rss": "^1.0.3",
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.0",
    "axios": "^0.18.0",
    "concurrent-transform": "^1.0.0",
    "gm": "^1.23.1",
    "gravatar": "^1.6.0",
    "gulp": "^4.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-image-resize": "^0.13.0",
    "gulp-rename": "^1.3.0",
    "gulp-sass": "^4.0.1",
    "gulp-serve": "^1.4.0",
    "gulp-shell": "^0.6.5",
    "gulp-uglify": "^3.0.1",
    "install": "^0.12.2",
    "markdown-it-footnote": "^3.0.1",
    "netlify-lambda": "^1.1.0",
    "pump": "^3.0.0",
